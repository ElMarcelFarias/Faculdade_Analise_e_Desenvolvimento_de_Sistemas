		int quantidadeHoras = sc.nextInt();
		
		if(quantidadeHoras > 40 ) {
			quantidadeHoras = quantidadeHoras - 40;
			
			
			valorSalarioMes = (valorHora * 40)*4;
			
		} else {
			
		}
	}

}
