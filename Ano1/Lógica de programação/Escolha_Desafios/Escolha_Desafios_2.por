programa
{
	
	funcao inicio()
	{
		inteiro ultimoNumeroPlaca

		escreva("Software de rodizio de placa! \n")
		escreva("Qual o último número da placa ")
		leia(ultimoNumeroPlaca)

		escolha(ultimoNumeroPlaca) {
			caso 0: 
				escreva("Você não pode rodar é Segunda-feira")
				pare
			caso 1: 
				escreva("Você não pode rodar é Terça-feira")
				pare
			caso 2: 
				escreva("Você não pode rodar é Quarta-feira")
				pare
			caso 3: 
				escreva("Você não pode rodar é Quinta-feira")
				pare
			caso 4: 
				escreva("Você não pode rodar é Sexta-feira")
				pare
			caso 5: 
				escreva("Você não pode rodar é Segunda-feira")
				pare
			caso 6: 
				escreva("Você não pode rodar é Terça-feira")
				pare
			caso 7: 
				escreva("Você não pode rodar é Quarta-feira")
				pare
			caso 8: 
				escreva("Você não pode rodar é Quinta-feira")
				pare
			caso 9: 
				escreva("Você não pode rodar é Sexta-feira")
				pare
			caso contrario:
				escreva("Valor inválido")
		}
	}
}
